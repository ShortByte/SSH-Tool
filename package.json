{
  "name": "ssh-tool",
  "productName": "SSH-Tool",
  "version": "0.0.5",
  "description": "Simple ssh terminal opener an manager",
  "homepage": "https://enneken-solutions.de",
  "repository": "git@github.com:ShortByte/SSH-Tool.git",
  "author": {
    "name": "Leon Enneken",
    "email": "kontakt@enneken-solutions.de"
  },
  "keywords": [],
  "build": {
    "appId": "ssh-tool",
    "asar": true,
    "directories": {
      "output": "out"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "dist/assets/icons/favicon.ico"
    },
    "nsis": {
      "installerIcon": "dist/assets/icons/favicon.ico",
      "uninstallerIcon": "dist/assets/icons/favicon.ico",
      "uninstallDisplayName": "SSH-Tool",
      "license": "license.txt",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "main": "main.js",
  "private": true,
  "scripts": {
    "build-installer": "electron-builder",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --x64",
    "publish": "electron-builder --x64 -p always",
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve -c web -o",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:build": "npm run build:prod && electron-builder build",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "10.0.1",
    "@angular-devkit/build-angular": "0.1001.3",
    "@angular-eslint/builder": "0.4.0-beta.2",
    "@angular-eslint/eslint-plugin": "0.4.0-beta.2",
    "@angular-eslint/eslint-plugin-template": "0.4.0-beta.2",
    "@angular-eslint/template-parser": "0.4.0-beta.2",
    "@angular/cli": "10.1.3",
    "@angular/common": "10.1.3",
    "@angular/compiler": "10.1.3",
    "@angular/compiler-cli": "10.1.3",
    "@angular/core": "10.1.3",
    "@angular/forms": "10.1.3",
    "@angular/language-service": "10.1.3",
    "@angular/platform-browser": "10.1.3",
    "@angular/platform-browser-dynamic": "10.1.3",
    "@angular/router": "10.1.3",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@types/jasmine": "3.5.14",
    "@types/jasminewd2": "2.0.8",
    "@types/mocha": "8.0.3",
    "@types/node": "12.12.6",
    "@types/ping": "0.2.0",
    "@typescript-eslint/eslint-plugin": "4.2.0",
    "@typescript-eslint/eslint-plugin-tslint": "4.2.0",
    "@typescript-eslint/parser": "4.2.0",
    "chai": "4.2.0",
    "conventional-changelog-cli": "2.1.0",
    "core-js": "3.6.5",
    "cross-env": "7.0.2",
    "electron": "10.1.2",
    "electron-builder": "22.8.1",
    "electron-reload": "1.5.0",
    "eslint": "7.10.0",
    "eslint-plugin-import": "2.22.1",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "5.2.3",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "6.3.1",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "mocha": "8.3.2",
    "npm-run-all": "4.1.5",
    "rxjs": "6.6.3",
    "spectron": "12.0.0",
    "ts-node": "9.0.0",
    "tslib": "2.0.1",
    "typescript": "3.9.7",
    "wait-on": "5.0.1",
    "webdriver-manager": "12.1.7",
    "zone.js": "0.10.3"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "dependencies": {
    "@sweetalert2/theme-dark": "4.0.3",
    "animate.css": "4.1.1",
    "bootstrap": "4.6.0",
    "electron-log": "4.3.2",
    "electron-updater": "4.3.8",
    "jquery": "3.6.0",
    "ping": "0.4.0",
    "popper.js": "1.16.1",
    "sweetalert2": "10.15.5"
  }
}
